'name': 'MineTweaker'
'scopeName': 'source.minetweaker'
'fileTypes': [
  'zs'
]
'patterns': [
  'semicolon': {
    'name': 'semicolon.minetweaker'
    'match': ';'
  }
  'functions':{
    'name': 'functions.minetweaker'
    'match': '\\b(print|recipes|addShaped|remove|addShapeless|addShapedMirrored|removeShaped|removeShapeless|furnace|setFuel|add|addAll|mirror|withDamage|withTag|onlyWithTag|onlyDamaged|onlyDamageAtLeast|onlyDamageAtMost|onlyDamageBetween|transformDamage|transformReplace|giveBack|noReturn|displayName|addTooltip|addShiftTooltip|addChestLoot|addSeed|removeSeed|setLocalization)\\b'
  }
  'stuff': {
    'name': 'stuff.minetweaker'
    'match': '\\b(for|in|val)\\b'
  }
  'styles': {
    'name': 'styles.minetweaker'
    'match': '\\b(format|italic|strikethrough)\\b'
  }
  'import': {
    'name': 'import.minetweaker'
    'match': '\\bimport\\b'
  }
  'grammar':{
    'name': 'grammar.minetweaker'
    'match': '\\b(.|"|<|>|:|[|])\\b'
  }
]
